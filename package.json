{
  "name": "ymReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint:create": "eslint --init",
    "lint": "eslint --ext .js ./src --fix",
    "ios": "NODE_ENV=dev npm run lint && react-native run-ios",
    "android": "NODE_ENV=dev npm run lint && react-native run-android",
    "ios-max": "npm run standard && react-native run-ios --simulator='iPhone X'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "prettier": "prettier src/**/**/*.js --write",
    "standard": "standard --fix && standard --verbose | snazzy",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commitmsg": "validate-commit-msg"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/node_modules",
      "/ios",
      "/landroid",
      "/tools",
      "/__tests__"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-native": "0.50.1",
    "react-navigation": "^1.0.0-beta.21",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.5",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "21.2.1",
    "react-test-renderer": "16.0.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "validate-commit-msg": "^2.14.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
